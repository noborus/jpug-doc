   <sect3 id="release-16-server-apps">
<!--
    <title>Server Applications</title>
-->
    <title>サーバアプリケーション</title>

    <itemizedlist>

<!--
Author: Tom Lane <tgl@sss.pgh.pa.us> 2023-03-22
     [3e51b278d] Add "-c name=value" switch to initdb.
